{
  "private": true,
  "name": "bleafsi-guest-open-api-base-sample",
  "version": "1.0.0",
  "description": "Sample OpenAPI application base with BLEA FSI",
  "license": "MIT-0",
  "bin": {
    "bleafsi-guest-openapi-base-sample": "bin/bleafsi-guest-openapi-base-sample.js"
  },
  "scripts": {
    "synth:dev": "npx cdk synth -c environment=dev",
    "depcheck": "npx depcheck --ignore-dirs cdk.out",
    "build": "tsc --build",
    "clean": "tsc --build --clean && rm -rf cdk.out",
    "watch": "tsc -w",
    "test": "jest --passWithNoTests",
    "lint": "eslint --fix .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "aws-cdk": "^2.83.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "prettier": "^2.7.1",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.83.0",
    "constructs": "^10.1.115",
    "source-map-support": "^0.5.21"
  }
}
