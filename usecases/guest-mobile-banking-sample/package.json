{
  "private": true,
  "name": "guest-mobile-banking-sample",
  "version": "1.0.0",
  "description": "モバイルバンキングアプリケーションサンプル",
  "license": "MIT-0",
  "bin": {
    "guest-mobile-banking-sample": "bin/app.js"
  },
  "scripts": {
    "synth:dev": "npx cdk synth",
    "depcheck": "npx depcheck --ignore-dirs cdk.out",
    "build": "tsc --build && npm run build:frontend",
    "build:frontend": "cd lib/frontend && npm install && npm run build",
    "clean": "tsc --build --clean && rm -rf cdk.out && rm -rf lib/frontend/build",
    "watch": "tsc -w",
    "test": "jest --passWithNoTests",
    "lint": "cd ../.. && eslint --fix usecases/guest-mobile-banking-sample",
    "format": "prettier --write .",
    "cdk": "cdk",
    "bootstrap": "cdk bootstrap",
    "deploy": "npm run build && cdk deploy --all",
    "destroy": "cdk destroy --all",
    "diff": "cdk diff"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "18.17.6",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
