    "aws-cdk": "^2.1006.0",
    "cdk-nag": "^2.22.33",
    "esbuild": "^0.25.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "overrides": {
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-api-gateway": "^3.896.0",
    "@aws-sdk/client-dynamodb": "^3.896.0",
    "@aws-sdk/client-eventbridge": "^3.896.0",
    "@aws-sdk/client-secrets-manager": "^3.896.0",
    "@aws-sdk/client-ses": "^3.896.0",
