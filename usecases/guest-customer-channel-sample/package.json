    "jest": "^27.5.1",
    "prettier": "^2.7.1",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.3",
    "@types/aws-lambda": "^8.10.106",
    "esbuild": "^0.15.9"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.43.1",
    "constructs": "^10.1.99",
    "source-map-support": "^0.5.21",
